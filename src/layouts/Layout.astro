---
import "../styles/global.css";
import "@fontsource/poppins";
import Head from "../components/Head.astro";
import Header from "../components/Header.astro";
import "lucide-astro";

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
  <Head title={title} />
  <body class="max-w-4xl w-11/12 mx-auto bg-whiteBg">
    <Header />
    <slot />
  </body>
</html>
